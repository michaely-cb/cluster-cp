        },
        "textMode": "auto",
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "pluginVersion": "10.1.0",
      "targets": [
        {
          "datasource": {
            "uid": "$datasource"
          },
          "expr": "sum((max(system_overall_health) by (system)) * on (system) group_left group by (system) (system_label{namespace=~\"$namespace\"}) == bool 1) + sum((max(system_overall_health) by (system)) * on (system) group_left group by (system) (system_label{namespace=~\"$namespace\"}) == bool 2)",
          "format": "time_series",
          "intervalFactor": 2,
          "legendFormat": "Available",
          "refId": "A"
        },
        {
          "datasource": {
            "uid": "$datasource"
          },
          "expr": "clamp_max(avg_over_time((sum((group(wsjob_system_grant{namespace=~\"$namespace\"}) by (system)) or (sum(0*system_label) by (system))) / count(group(system_label{namespace=~\"$namespace\"}) by (system)))[$__range:$__rate_interval]) *100, 100)",
          "format": "time_series",
          "intervalFactor": 2,
          "legendFormat": "Utilized Percent",
          "refId": "B"
        },
        {
          "datasource": {
            "uid": "$datasource"
          },
          "expr": "avg_over_time((sum(group(wsjob_system_grant{namespace=~\"$namespace\"}) by (system)) or sum(sum(system_label*0) by (system)))[$__range:$__rate_interval]) * (${__to:date:seconds}-${__from:date:seconds})/3600",
          "format": "time_series",
          "intervalFactor": 2,
          "legendFormat": "Utilized Hours",
          "refId": "C"
        }
      ],
      "title": "CS2 Overall Status",
      "type": "stat"
