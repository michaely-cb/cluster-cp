      },
      "pluginVersion": "11.3.1",
      "targets": [
        {
          "datasource": {
            "uid": "$datasource"
          },
          "editorMode": "code",
          "exemplar": true,
          "expr": "sum(rate(container_cpu_usage_seconds_total{namespace=\"loki\", container=\"loki\", pod=~\"loki-.*\"}[1m])) by (pod)",
          "hide": false,
          "interval": "",
          "legendFormat": "Loki Usage",
          "range": true,
          "refId": "A"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
          },
          "editorMode": "code",
          "expr": "machine_cpu_cores\n    * on(node)\n    group_left(pod)\n    kube_pod_info{namespace=\"loki\", pod=~\"loki-.*\"}",
          "hide": false,
          "instant": false,
          "legendFormat": "Total Cores",
          "range": true,
          "refId": "B"
        }
      ],
      "title": "Loki CPU Usage",
      "type": "timeseries"
    },
