{
  "cniVersion": "0.3.1",
  "name": $name,
  "type": "macvlan",
  "master": $nic,
  "mode": "bridge",
  "ipam": {
    "type": "whereabouts",
    "log_level": "debug",
    "log_file": "/var/log/whereabouts.log",
    "range": $parentnet,
    "range_start": $start_ip,
    "range_end": $end_ip,
    "routes": [
      {
        "dst": $subnet,
        "gw": "0.0.0.0"
      }
    ]
  }
}